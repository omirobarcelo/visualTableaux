	public static final String ARROW_RIGHT = "\u2192";
	public static final String ARROW_DOWN = "\u2193";
	
	public static final String OP_TOP = "\u22A4";
	public static final String OP_BOTTOM = "\u22A5";
	public static final String OP_COMPLEMENT = "Â¬";
	public static final String OP_UNION = "\u2A06";
	public static final String OP_INTERSECTION = "\u2A05";
	public static final String OP_EXISTENTIAL = "\u2203";
	public static final String OP_UNIVERSAL = "\u2200";
}
