package ver1;

public class OWNUnion implements OWNAxiom {
	
	private AXIOM_TYPE type;
	private OWNAxiom operand1;
	private OWNAxiom operand2;
	
	public OWNUnion(OWNAxiom op1, OWNAxiom op2) {
		this.type = AXIOM_TYPE.UNION;
		this.operand1 = op1;
		this.operand2 = op2;
	}

	public OWNAxiom getOperand1() {
		return operand1;
	}

	public OWNAxiom getOperand2() {
		return operand2;
	}
	
	public boolean isOfType() {
		return true;
	}

}
