package ver1;

public interface OWNAxiom {
	public enum AXIOM_TYPE {UNION, INTERSECTION, EXISTENTIAL, UNIVERSAL}
	
	public AXIOM_TYPE type = null;
	public OWNAxiom operand1 = null;
	public OWNAxiom operand2 = null;
	
	public AXIOM_TYPE getType();
	public OWNAxiom getOperand1();
	public OWNAxiom getOperand2();
	
	public boolean isOfType();
}
